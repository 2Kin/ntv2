{% extends "FOSUserBundle::layout.html.twig" %}

{% block fos_user_content %}
<div class="fos_user_group_list">
    <ul>
    {% for group in groups %}
        <li><a href="{{ path('fos_user_group_show', {'groupname': group.getName()} ) }}">{{ group.getName() }}</a></li>
    {% endfor %}
    </ul>
</div>
{% endblock fos_user_content %}
